      <bind nodeset="/mobile_campaign/events/one_part" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/group_session" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/group_part" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/lla_meeting" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/lla_part" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/coun_num" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/coun_part" required="true()" type="int"/>
      <bind nodeset="/mobile_campaign/events/iec_dist" required="true()" type="int"/>
